<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eleves.Bibliotheque">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/book_ico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/book_ico"
        android:supportsRtl="true"
        android:theme="@style/myTheme">
        <activity android:name="com.example.eleves.Bibliotheque.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.eleves.Bibliotheque.LivreActivity"></activity>
    </application>

</manifest>